alias aws_creds="edit ~/.aws/credentials"
alias aws_config="edit ~/.aws/config"
alias aws_info="aws sts get-caller-identity"

# AWS Vault
alias av='aws-vault'
alias avl='aws-vault login'

# SSM start-sessions
alias gow_sesh='avgow aws ssm start-session --target'
alias nk_sesh='avnk aws ssm start-session --target'
